//Add popup functionality to the layer
function onEachFeature1(feature, layer) {
    {
        layer.bindPopup("<b>Segment:</b> " +feature.properties.Segment + "<br><b>Option(s):</b> " + feature.properties.Option);
    }
}
function onEachFeature2(feature, layer) {
    {
        layer.bindPopup("<b>Substation:</b> " +feature.properties.Type);
    }
}
// Style alignment GeoJSON based on the attribute fields
L.geoJson(alignment, {color: "#FF0000", opacity:1}).addTo(map);
L.geoJson(alignment, { onEachFeature:onEachFeature1,
    style: function(feature) {
        switch (feature.properties.Segment) {
            //case 'Bellevue South': return {color: "#ABB14F", opacity: 1};
            case 'Redmond': return {color: "#F1BF58", opacity: 1};
            case 'Bellevue North': return {color: "#F04D24", opacity: 1};
            case 'Newcastle': return {color: "#455C89", opacity: 1};
            case 'Renton': return {color: "#9950A4", opacity: 1};
            //case 'Bellevue Central': return {color: "#7c481f", opacity: 1};
        }
        switch(feature.properties.Option){
        	case 'Existing Corridor': return {color: "#7C481F", opacity: 1};
        	case 'Bypass 1 | Existing Corridor': return {color: "#70411C", opacity: 1};
        	case 'Bypass 1': return {color: "#B09077", opacity: 1};
        	case 'Bypass 2': return {color: "#A37E61", opacity: 1};
        	case 'Bypass 1 | Bypass 2': return {color: "#643A19", opacity: 1};
        	case 'Bypass 1 | Bypass 2 | Existing Corridor': return {color: "#895A35", opacity: 1};

        	case 'Willow 1 | Willow 2 | Oak 1 | Oak 2': return {color: "#ABB14F", opacity: 1};
        	case 'Willow 2': return {color: "#9A9F47", opacity: 1};	
        	case 'Oak 1 | Oak 2': return {color: "#898D3F", opacity: 1};	
        	case 'Oak 2': return {color: "#C3C985", opacity: 1};
        	case 'Oak 1 | Oak 2 | Willow 2': return {color: "#CBD197", opacity: 1};		
        }
    }
}).addTo(map);
//add substations
var geojsonMarkerOptions = {
    radius: 11,
    fillColor: "#000000",
    color: "#ffffff",
    weight: 1,
    opacity: 1,
    fillOpacity: 0.8
};

var station = L.geoJson(substations, { onEachFeature:onEachFeature2,
    pointToLayer: function (feature, latlng) {
        return L.circleMarker(latlng, geojsonMarkerOptions );
    }
}); var MyCustomMarker = L.Icon.extend({
        options: {
            shadowUrl: null,
            iconAnchor: new L.Point(12, 12),
            iconSize: new L.Point(24, 24),
            iconUrl: 'link/to/image.png'
        }
    });
station.addTo(map)
//Initialize the FeatureGroup to store editable layers
var drawnItems = new L.FeatureGroup();
map.addLayer(drawnItems);

var MyCustomMarker = L.Icon.extend({
        options: {
            shadowUrl: null,
            iconAnchor: new L.Point(12, 12),
            iconSize: new L.Point(24, 24),
            iconUrl: 'link/to/image.png'
        }
    });

// Initialise the draw control and pass it the FeatureGroup of editable layers
var options = {
	position: 'topright',
	draw: {
		polygon: false,
		polyline: false,
		rectangle: false,
		circle: false,
		},
		
    edit: {
        featureGroup: drawnItems,
        edit:false,
        remove:false
    	}

};
var drawnControl = new L.Control.Draw({
	position: 'topright',
	draw: {
		polygon: false,
		polyline: false,
		rectangle: false,
		circle: false,
		marker:false,
		},
		
    edit: {
        featureGroup: drawnItems,
        remove:false
    	}

});
var drawControl = new L.Control.Draw(options);
map.addControl(drawControl);

// adds drawn points to the drawnItems layer
map.on('draw:created', function (e) {
        var type = e.layerType,
            layer = e.layer;
		drawnItems.addLayer(layer);
		map.removeControl(drawControl);
		map.addControl(drawnControl);

	});

//the initialize funcction loades the lat/long from the drawn item
	function initialize(){
		$.each( drawnItems._layers, function( key, value ) {
  		var _lat =  drawnItems._layers[key]._latlng.lat;
  		var _lng =  drawnItems._layers[key]._latlng.lng;
		document.getElementById("entry_1843466499").value = _lat;
		document.getElementById("entry_2095728529").value = _lng;
	});
}
					
//for changing the thank-you page
var submitted=false
 
// This function will be called after the Google Script has executed
  function fileUploaded(status) {
    document.getElementById('ss-form').style.display = 'none';
    document.getElementById('output').innerHTML = status;
  }
  
